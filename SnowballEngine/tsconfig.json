{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES2015",
    "lib": [ "ESNext", "DOM" ],
    "sourceMap": false,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "stripInternal": true,
    "outDir": "dist"
  },
  "exclude": [
    "node_modules"
  ],
  "typedocOptions": {
    "mode": "modules",
    "out": "docs"
  }
}